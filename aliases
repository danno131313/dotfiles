#!/bin/bash
alias pacin='yaourt -Sa '
alias pacup='yaourt -Sayu '
alias pacrm='sudo pacman -Rns '
alias untar='tar -xvzf '
alias ,='cd ..'
alias ,,='cd ../..'
alias ,,,='cd ../../..'
alias fixgpg='sudo pacman-key --init && sudo pacman-key --populate archlinux && sudo pacman -Syy'
alias ftb='java -jar ~/ftb/FTB_Launcher.jar'
alias mute='amixer -q sset Master mute'
alias hphones='pacmd set-sink-port 1 analog-output-headphones && amixer -q sset Master 25%'
alias speakers='pacmd set-sink-port 1 analog-output-speaker && amixer -q sset Master 45%'
alias unmute='amixer -q sset Master 25% unmute'
alias music='hphones && amixer -q sset Master 100%'
alias hdmi='xrandr --output HDMI2 --auto && pacmd set-default-sink 0 && amixer -q sset Master 15% unmute'
alias pacclean='paccache -r; paccache -ruk0 && sudo pkgcacheclean'
alias duall='du -sch .[!.]* * |sort -h'
alias nrmlaux='pacmd set-default-sink 1 && hphones'
alias tv='xrandr --output HDMI2 --auto && pacmd set-sink-port 1 analog-output-headphones && amixer -q sset Master 100% unmute'
alias viewpic='feh -. '
alias noblank='xset s off -dpms'
alias gparted='sudo gparted; exit'
alias screenfetch="screenfetch -t"
alias gputest='primusrun glxspheres64'
alias vi='nvim'
alias din='sudo dnf install'
alias dup='sudo dnf distro-sync'
alias cs='cd ~/CSCI/CS-241/'
alias sd='shutdown now'
alias xup='xrdb ~/.Xresources'
alias ls='ls --color=auto'
alias mkdir='mkdir -pv'
alias fixsteam='find ~/.local/share/Steam/ \( -name "libgcc_s.so*" -o -name "libstdc++.so*" -o -name "libxcb.so*" -o -name "libgpg-error.so*" \) -print -delete'
alias headphones='sudo python /opt/headphones/Headphones.py'
alias startvpn='sudo systemctl start openvpn@US_Seattle'
alias stopvpn='sudo systemctl stop openvpn@US_Seattle'
alias aptin='sudo apt install'
alias aptup='sudo apt update && sudo apt upgrade'
alias reflect='sudo reflector --verbose --score 200 -f 20 -a 2 --sort rate --save /etc/pacman.d/mirrorlist'
alias sysen='sudo systemctl enable'
alias sysdis='sudo systemctl disable'
alias thunar='thunar; exit'
alias config='git --git-dir=$HOME/.cfg/ --work-tree=$HOME'
alias svi='sudo nvim'
alias clear='clear && neofetch'
alias fixwifi='sudo modprobe -r mwifiex_pcie && sudo echo 1 > /sys/bus/pci/devices/0000\:02\:00.0/reset && sudo modprobe mwifiex_pci'
alias rsync='rsync -av'
alias eclipse='SWT_GTK3=0 eclipse </dev/null &>/dev/null &'
alias spring='SWT_GTK3=0 ~/Documents/sts-3.9.1.RELEASE/STS'
alias em='emacsclient -nc -s instance1'
alias ~='cd ~'
alias mn='cd ~/DojoAssignments/MEAN/'
function newdj() { ~/bin/django_helper/django-project $1 $2 && cd ~/DojoAssignments/Python/Django/$1;}
function newdjapp() { ~/bin/django_helper/django-app $1 && cd ./$1;}
function mkusb() { sudo dd bs=4M if=$1 of=$2 && sync; }
